          "type": "basic"
        },
        {
          "from": {
            "key_code": "close_bracket",
            "modifiers": {
              "mandatory": [
                "left_shift",
                "left_command",
                "left_control",
                "left_option"
              ],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "end"
            }
          ],
          "type": "basic"
        },
        {
          "description": "Avoid starting sysdiagnose with the built-in macOS shortcut cmd+shift+option+ctrl+,",
          "from": {
            "key_code": "comma",
            "modifiers": {
              "mandatory": ["command", "shift", "option", "control"]
            }
          },
          "to": [],
          "type": "basic"
        },
        {
          "description": "Avoid starting sysdiagnose with the built-in macOS shortcut cmd+shift+option+ctrl+.",
